<h1>Procurement List</h1>
<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Author</th>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <% current_user.selected.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.author %></td>
      <td><%= link_to 'Show', book %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<%= link_to 'Back', root_path %>